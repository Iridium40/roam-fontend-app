{
  "headers": [
    {
      "source": "/api/notifications/edge",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PATCH, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Cache-Control"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/notifications/edge",
      "destination": "/api/notifications/edge.ts"
    },
    {
      "source": "/api/bookings/status-update",
      "destination": "/api/bookings/status-update.ts"
    }
  ]
}